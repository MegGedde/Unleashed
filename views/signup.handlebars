{message: req.flash(‘error’)}
<!-- SIGN UP FORM -->
<form class="m-4 is-flex is-flex-direction-column is-align-items-center login-form" id="#sign-up">
    <div class="form-header">
        <h2>SIGN UP</h2>
    </div>
    <div class="m-3">
        <label>Username</label>
        <input type="text" placeholder="John1234" id="username-signup">
        <label>Password</label>
        <input type="password" placeholder="*******" id="password-signup">
        <label>Email Address</label>
        <input type="email" placeholder="e.g. alex@example.com" id="email-signup">
    </div>
    <button class="m-0 btn-generic" type="submit">Sign Up!</button>
</form>
    <script src="javascript/signup.js"></script> 